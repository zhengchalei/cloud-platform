specification SysDictPageSpecification {
    eq(id)
    like/i(name)
    like/i(code)
    like/i(description)
    eq(status)
}

SysDictPageView {
    #allScalars
}

SysDictDetailView {
    #allScalars
    dictItems {
        #allScalars
    }
}

input SysDictCreateInput {
    #allScalars(this)
    -id
}

input SysDictUpdateInput {
    #allScalars(this)
    id!
}
