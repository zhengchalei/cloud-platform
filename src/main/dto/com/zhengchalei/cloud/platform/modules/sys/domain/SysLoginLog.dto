specification SysLoginLogPageSpecification {
    eq(id)
    like/i(username)
    like/i(password)
    like/i(ip)
    like/i(address)
    eq(status)
    like/i(errorMessage)
    ge(loginTime) as startTime
    le(loginTime) as endTime
}

